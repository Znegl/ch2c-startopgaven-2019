{
  "version": 2,
  "routes": [
    {
      "src": "/public/(.*)",
      "dest": "/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/",
      "status": 301,
      "headers": {
        "Location": "/1"
      }
    },
    {
      "src": "/.*",
      "status": 404,
      "dest": "/404.html"
    }
  ]
}